//text
print("The Love Calculator is calculating your score...")
name1 = input("What is your full name \n") # What is your name?
name2 = input("What is the full name of your love \n") # What is their name?

names = name1.lower() + name2.lower()

t = names.count('t')
r = names.count('r')
u = names.count('u')
e = names.count('e')
total_true = int(t+r+u+e)

l = names.count('l')
o = names.count('o')
v = names.count('v')
e = names.count('e')
total_love = int(l+o+v+e)

index = int(str(total_true) +str(total_love))

//simple comment
if index < 10 or index > 90:
  print(f"Your score is {index}, you go together like coke and mentos.")
if index > 40 and index < 50:
  print(f"Your score is {index}, you are alright together.")
else:
  print(f"Your score is {index}.")
